@model Setting
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/admin/Views/Shared/_Layout.cshtml";
}


<div class="container">
    <div class="row">
        <div class="col-lg-6">
            <form asp-area="Admin" asp-controller="Setting" asp-action="Create" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for="@Model.LogoFile">Logo</label>
                    <input class="form-control" asp-for="@Model.LogoFile">
                    <span asp-validation-for="@Model.LogoFile" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.Phone">Phone</label>
                    <input class="form-control" asp-for="@Model.Phone">
                    <span asp-validation-for="@Model.Phone" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.Mail">Mail</label>
                    <input class="form-control" asp-for="@Model.Mail">
                    <span asp-validation-for="@Model.Mail" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.Address">Address</label>
                    <input class="form-control" asp-for="@Model.Address">
                    <span asp-validation-for="@Model.Address" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.CopyrightLink">CopyrightLink</label>
                    <input class="form-control" asp-for="@Model.CopyrightLink">
                    <span asp-validation-for="@Model.CopyrightLink" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.Map">Map</label>
                    <input class="form-control" asp-for="@Model.Map">
                    <span asp-validation-for="@Model.Map" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.ContactText">ContactText</label>
                    <textarea id="editor" class="form-control" asp-for="@Model.ContactText"></textarea>
                    <span asp-validation-for="@Model.ContactText" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.FAQText">FAQText</label>
                    <input class="form-control" asp-for="@Model.FAQText">
                    <span asp-validation-for="@Model.FAQText" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.VideoLink">VideoLink</label>
                    <input class="form-control" asp-for="@Model.VideoLink">
                    <span asp-validation-for="@Model.VideoLink" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.VideoLink2">VideoLink2</label>
                    <input class="form-control" asp-for="@Model.VideoLink2">
                    <span asp-validation-for="@Model.VideoLink2" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.ServiceVideo">ServiceVideo</label>
                    <input class="form-control" asp-for="@Model.ServiceVideo">
                    <span asp-validation-for="@Model.ServiceVideo" class="text-danger"></span>
                </div>
                <button type="submit" class="btn btn-primary">Create</button>
                <button type="submit" class="btn btn-dark" asp-area="admin" asp-controller="Setting" asp-action="Index">Back to List</button>
            </form>
        </div>
    </div>
</div>